set number
let mapleader = "<Space>"
filetype plugin indent on 
syntax on 
set encoding=utf-8
set hls
set tabstop=4
set shiftwidth=4
set noexpandtab
colorscheme darkblue

nnoremap <C-J> <C-W><C-J>
nnoremap <C-K> <C-W><C-K>
nnoremap <C-H> <C-W><C-H>
nnoremap <C-L> <C-W><C-L>
nnoremap <C-X> <C-W>x

set guifont=Menlo\ Regular:h13
